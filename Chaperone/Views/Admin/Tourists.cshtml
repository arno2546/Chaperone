@model IEnumerable<Chaperone.Models.User>

@{
    ViewBag.Title = "Tourists";
    Layout = "~/Views/Shared/_AdminDashLayout.cshtml";
}

<h3>Tourists</h3>
<hr />

<p>
  
</p>
<h2 style="text-align:center">Tourists</h2>
<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.User_type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Contact)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.status)
        </th>
       

        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Email)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.User_type)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Contact)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.status)
    </td>
    <td>
        @Html.ActionLink("Ban", "deleteGenUser", new { id = item.Id })
    </td>
</tr>
}

</table>
